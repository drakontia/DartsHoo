<p id="notice"><%= notice %></p>

<h1>Regs</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Gameid</th>
      <th>Regno</th>
      <th>Gametype</th>
      <th>Gamestats</th>
      <th>Roundnum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regs.each do |reg| %>
      <tr>
        <td><%= reg.userid %></td>
        <td><%= reg.gameid %></td>
        <td><%= reg.regno %></td>
        <td><%= reg.gametype %></td>
        <td><%= reg.gamestats %></td>
        <td><%= reg.roundnum %></td>
        <td><%= link_to 'Show', reg %></td>
        <td><%= link_to 'Edit', edit_reg_path(reg) %></td>
        <td><%= link_to 'Destroy', reg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reg', new_reg_path %>
